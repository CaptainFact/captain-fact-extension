{
  "version": "0.8.0",
  "name": "CaptainFact (Staging)",
  "manifest_version": 2,
  "key": "",
  "description": "Collaborative, real-time fact checking. Free and open source.",
  "applications": {
    "gecko": {"id": "extension-staging@captainfact.io"}
  },
  "browser_action": {
    "default_icon": {
      "16": "img/icon-16.png",
      "48": "img/icon-48.png",
      "128": "img/icon-128.png"
    },
    "default_title": "CaptainFact",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "web_accessible_resources": [
    "inject.html",
    "img/*.png",
    "img/*.svg"
  ],
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "content_scripts": [{
    "matches": ["https://staging.captainfact.io/*"],
    "js": ["/js/installation_notifier.bundle.js"]
  }],
  "permissions": [ "tabs", "storage", "*://www.youtube.com/*", "*://localhost/*", "*://*.staging.captainfact.io/*" ],
  "content_security_policy": "default-src 'self'; script-src 'self'; style-src * 'unsafe-inline'; img-src 'self' data:; connect-src 'self' https://*.staging.captainfact.io:*;"
}
